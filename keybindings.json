// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+shift+t",
    "command": "workbench.action.addRootFolder"
  },
  {
    "key": "ctrl+cmd+left",
    "command": "workbench.action.moveEditorToPreviousGroup",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+cmd+left",
    "command": "-workbench.action.moveEditorToPreviousGroup"
  },
  {
    "key": "ctrl+cmd+left",
    "command": "workbench.action.increaseViewWidth",
    "when": "sideBarFocus"
  },
  {
    "key": "ctrl+cmd+right",
    "command": "workbench.action.decreaseViewWidth",
    "when": "sideBarFocus"
  },
  {
    "key": "shift+cmd+c",
    "command": "-workbench.action.terminal.openNativeConsole",
    "when": "!terminalFocus"
  },
  {
    "key": "shift+cmd+c",
    "command": "workbench.view.extension.codium-sidebar-view-container"
  },
  {
    "key": "cmd+l",
    "command": "workbench.action.quickOpenWithModes"
  },
  {
    "key": "ctrl+cmd+r",
    "command": "workbench.action.evenEditorWidths"
  },
  {
    "key": "ctrl+cmd+f",
    "command": "-workbench.action.toggleFullScreen",
    "when": "!isIOS"
  },
  {
    "key": "ctrl+cmd+f",
    "command": "workbench.action.toggleEditorWidths"
  },
  {
    "key": "ctrl+shift+o",
    "command": "workbench.action.openWorkspace"
  },
  {
    "key": "alt+cmd+backspace",
    "command": "workbench.action.remote.close",
    "when": "tunnelCloseable && tunnelViewFocus"
  },

  {
    "key": "alt+cmd+t",
    "command": "-workbench.action.closeOtherEditors"
  },
  {
    "key": "shift+cmd+k",
    "command": "extension.multiCommand.execute",
    "args": {
      "sequence": [
        "workbench.action.closeEditorsInOtherGroups",
        "workbench.action.closeOtherEditors"
      ]
    }
  },
  {
    "key": "ctrl+shift+=",
    "command": "extension.multiCommand.execute",
    "args": {
      "sequence": [
        "workbench.action.newGroupRight",
        "workbench.action.quickOpen"
      ]
    }
  },
  {
    "key": "cmd+k alt+cmd+left",
    "command": "-workbench.action.previousEditorInGroup"
  },
  {
    "key": "cmd+k alt+cmd+right",
    "command": "-workbench.action.nextEditorInGroup"
  },
  {
    "key": "ctrl+shift+0",
    "command": "workbench.action.focusActiveEditorGroup"
  },
  {
    "key": "ctrl+shift+9",
    "command": "workbench.action.focusLastEditorGroup"
  },
  {
    "key": "ctrl+shift+[",
    "command": "workbench.action.focusPreviousGroup"
  },
  {
    "key": "ctrl+shift+]",
    "command": "workbench.action.focusNextGroup"
  },
  {
    "key": "shift+cmd+[",
    "command": "workbench.action.previousEditorInGroup"
  },
  {
    "key": "shift+cmd+[",
    "command": "-workbench.action.previousEditor"
  },
  {
    "key": "shift+cmd+]",
    "command": "workbench.action.nextEditorInGroup"
  },
  {
    "key": "shift+cmd+]",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "shift+cmd+n",
    "command": "explorer.newFolder",
    "when": "filesExplorerFocus"
  },
  {
    "key": "cmd+n",
    "command": "explorer.newFile",
    "when": "filesExplorerFocus"
  },
  {
    "key": "ctrl+[",
    "command": "workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+p",
    "command": "-workbench.action.quickOpenSelectPrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+]",
    "command": "workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+n",
    "command": "-workbench.action.quickOpenSelectNext",
    "when": "inQuickOpen"
  },
  {
    "key": "shift+cmd+o",
    "command": "git.openFile2",
    "when": "inDiffEditor",
  },
  {
    "key": "cmd+o",
    "command": "git.openFile",
    "when": "inDiffEditor"
  },
  {
    "key": "cmd+o",
    "command": "gitlens.openWorkingFileInDiffRight",
    "when": "gitlens:enabled && isInDiffRightEditor && resourceScheme =~ /^(gitlens|pr)$/"
  },
  {
    "key": "shift+cmd+k s",
    "command": "workbench.action.openGlobalKeybindingsFile",
    "when": "inKeybindings"
  },
  {
    "key": "ctrl+cmd+right",
    "command": "workbench.action.moveEditorToNextGroup",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+cmd+right",
    "command": "-workbench.action.moveEditorToNextGroup"
  },
  {
    "key": "cmd+w",
    "command": "workbench.action.closeActiveEditor",
    "when": "activeEditor"
  },
  {
    "key": "cmd+w",
    "command": "-workbench.action.closeActiveEditor"
  },
  {
    "key": "cmd+w",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+k cmd+w",
    "command": "workbench.action.terminal.killAll",
    "when": "terminalFocus"
  }
]
